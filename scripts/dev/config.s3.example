# Exemplo de configuração para usar S3 como storage do CodeLog

# Configurações básicas da aplicação
FLOWS_CODE_ACTIONS_HOST=:
FLOWS_CODE_ACTIONS_PORT=8050
FLOWS_CODE_ACTIONS_ENVIRONMENT=production
FLOWS_CODE_ACTIONS_LOG_LEVEL=info

# Configurações MongoDB (ainda necessário para outros dados)
FLOWS_CODE_ACTIONS_MONGO_DB_URI=mongodb://localhost:27017
FLOWS_CODE_ACTIONS_MONGO_DB_NAME=code-actions

# Configurações S3 para CodeLog
FLOWS_CODE_ACTIONS_S3_ENABLED=true
FLOWS_CODE_ACTIONS_S3_REGION=us-east-1
FLOWS_CODE_ACTIONS_S3_BUCKET_NAME=my-codeactions-logs
FLOWS_CODE_ACTIONS_S3_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE
FLOWS_CODE_ACTIONS_S3_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY
FLOWS_CODE_ACTIONS_S3_PREFIX=codeactions

# Configurações do limpador de logs
FLOWS_CODE_ACTIONS_CLEANER_SCHEDULE_TIME=01:00
FLOWS_CODE_ACTIONS_CLEANER_RETENTION_PERIOD=30

# Outras configurações
FLOWS_CODE_ACTIONS_REDIS=redis://localhost:6379/10
FLOWS_CODE_ACTIONS_CODE_LIMITER_MAX_REQUESTS=600
FLOWS_CODE_ACTIONS_CODE_LIMITER_WINDOW_WINDOW=60
