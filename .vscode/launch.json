  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/codeactions/main.go",
      "env": {
        "FLOWS_CODE_ACTIONS_OIDC_REALM": "weni",
        "FLOWS_CODE_ACTIONS_OIDC_HOST": "https://accounts.weni.ai/auth",
        "FLOWS_CODE_ACTIONS_AUTH_TOKEN": "",
        "FLOWS_CODE_ACTIONS_OIDC_AUTH_ENABLED": "false",
        // "FLOWS_CODE_ACTIONS_RABBITMQ_URL": "amqp://localhost:5672/",
        "FLOWS_CODE_ACTIONS_CLEANER_RETENTION_PERIOD": "1",
        "FLOWS_CODE_ACTIONS_CLEANER_SCHEDULE_TIME": "16:00",
        "FLOW_CODE_ACTIONS_PERMISSION_QUEUE": "code-actions.permissions",
        "FLOWS_CODE_ACTIONS_PERMISSION_EXCHANGE": "project-auths.topic",
        "FLOWS_CODE_ACTIONS_S3_ENABLED": "true",
        "FLOWS_CODE_ACTIONS_S3_ENDPOINT": "http://localhost:4566",
        "FLOWS_CODE_ACTIONS_S3_BUCKET_NAME": "codeactions-dev",
        "FLOWS_CODE_ACTIONS_S3_REGION": "us-east-1",
        "FLOWS_CODE_ACTIONS_S3_PREFIX": "codeactions",
        "FLOWS_CODE_ACTIONS_S3_ACCESS_KEY_ID": "test",
        "FLOWS_CODE_ACTIONS_S3_SECRET_ACCESS_KEY": "test",
        "FLOWS_CODE_ACTIONS_DB_TYPE": "postgres"
      }
    }
  ]
}